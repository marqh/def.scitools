@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix reg:     <http://purl.org/linked-data/registry#> .
@prefix ldp:     <http://www.w3.org/ns/ldp#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .

[] a reg:RegisterItem ;
    reg:itemClass foaf:Organization ;
    reg:definition [ reg:entity <scitools> ];
    reg:status reg:statusStable ;
    reg:notation "scitools" ;
    .
<scitools> a foaf:Organization ;
    rdfs:label "SciTools"@en ;
    dct:description "SciTools for Scientific Python Programmers."@en ;
    foaf:homepage <http://scitools.org.uk/> ;
    foaf:name "SciTools"
    .
    